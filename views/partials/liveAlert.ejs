<div id="liveAlertPlaceholder">
  <% if (messages.length !== 0) {%>
  <div class="alert alert-info alert-dismissible" role="alert">
    <%=messages%>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
  <% } %>
</div>
